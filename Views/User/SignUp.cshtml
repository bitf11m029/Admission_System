@model MvcApplication1.Models.UserData

@{
    ViewBag.Title = "SignUp";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

 

 <div class="container">
         <div class="row">
             <div class="center-block">
                 <div class="col-xs-12 col-sm-12 col-md-4 well well-sm">
                     <legend><a href="http://www.jquery2dotnet.com"><i class="glyphicon glyphicon-globe"></i></a> Sign up!</legend>
                     <form action="/User/SignUpNew" method="post" class="form" role="form">
                         @Html.AntiForgeryToken()
                         @Html.ValidationSummary(true)
                         <div class="row">
                             <div class="col-xs-6 col-md-6">
                                 @Html.TextBoxFor(x => x.FirstName, new { @class = "form-control", id = "fn", placeholder = "First Name", required = "required", autofocus = "autofocus" })
                             </div>
                             <div class="col-xs-6 col-md-6">
                                 @Html.TextBoxFor(x => x.LastName, new { @class = "form-control", id = "ln", placeholder = "Last Name", required = "required" })
                             </div>
                         </div>
                         <div id="x"></div>
                         <div id="checking">Checking...</div>
                         @Html.TextBoxFor(x => x.UserName, new { @class = "form-control", id = "un", placeholder = "User Name", required = "required" })
                         @Html.TextBoxFor(x => x.EmailID, new { @class = "form-control", id = "em", placeholder = "your email", required = "required" })
                         <input type="text" id="em1" placeholder="Re-Enter Email" required class="form-control" />
                         @Html.PasswordFor(x => x.Password, new { @class = "form-control", id = "pa", placeholder = "New Password", required = "required" })
                         <label for="">
                             Birth Date
                         </label>
                         <div class="container">
                             <div class="row">
                                 <div class="col-sm-4">
                                     <div class="date-picker" data-date="2014/09/02" data-keyboard="true">
                                         <div class="date-container pull-left">
                                             <h4 class="weekday">Monday</h4>
                                             <h4 class="date">Februray 4th</h4>
                                             <h4 class="year pull-right">2014</h4>
                                         </div>
                                         <span data-toggle="datepicker" data-type="subtract" class="fa fa-angle-left"></span>
                                         <span data-toggle="datepicker" data-type="add" class="fa fa-angle-right"></span>
                                         <div class="input-group input-datepicker">
                                             <input type="text" class="form-control" data-format="YYYY/MM/DD" placeholder="YYYY/MM/DD" name="DOB">
                                             <span class="input-group-btn">
                                                 <button class="btn btn-default" type="button">Go!</button>
                                             </span>
                                         </div>
                                         <span data-toggle="calendar" class="fa fa-calendar"></span>
                                     </div>
                                 </div>
                             </div>
                         </div>
                         <button class="btn btn-lg btn-primary btn-block" type="submit">
                             Sign up
                         </button>
                     </form>
                 </div>
             </div>
     </div>
</div>          
